{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutate": [
    "src/components/About.js",
    "src/App.js",
    "src/components/FavoritePokemons.js",
    "src/components/NotFound.js",
    "src/components/Pokedex.js",
    "src/components/Pokemon.js",
    "src/components/PokemonDetails.js"
  ],
 "mutator": {
    "name": "javascript",
    "excludedMutations": [
      "ArithmeticOperator",
      "ArrayDeclaration",
      "ArrowFunction",
      "BlockStatement",
      "BooleanLiteral",
      "ConditionalExpression",
      "EqualityOperator",
      "LogicalOperator",
      "ObjectLiteral",
      "UnaryOperator",
      "UpdateOperator"
    ]
  },
  "testRunner": "jest",
  "reporters": [
    "progress",
    "clear-text",
    "html"
  ],
  "coverageAnalysis": "off",
  "jest": {
    "projectType": "create-react-app"
  }
}
